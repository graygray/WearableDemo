	</RelativeLayout>
	
    <RelativeLayout
        android:id="@+id/relativeLayout_history22"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:layout_weight="1" >
        
    </RelativeLayout>
        
    <RelativeLayout
	    android:id="@+id/relativeLayout_history23"
	    android:layout_width="match_parent"
